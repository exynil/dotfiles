{
    "editor.fontFamily": "'JetBrains Mono', 'monospace', 'Droid Sans Fallback', 'Material Design Icons', 'Typicons', 'FontAwesome', 'Noto Color Emoji'",
    "files.autoSave": "onFocusChange",
    "workbench.sideBar.location": "left",
    "breadcrumbs.enabled": true,
    "editor.renderWhitespace": "all",
    "workbench.startupEditor": "newUntitledFile",
    "explorer.confirmDelete": false,
    "explorer.compactFolders": false,
    "editor.rulers": [
        60,
        80,
        120
    ],
    "workbench.iconTheme": "vscode-icons",
    "files.associations": {
        "**/templates/*.html": "django-html", // Для работы emmet в файлах Django
    },
    "emmet.includeLanguages": {
        "django-html": "html" // Для работы emmet в файлах Django
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.printWidth": 120,
    "workbench.colorTheme": "One Dark Pro",
    "editor.tabSize": 2,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}