{
  "editor.fontFamily": "'JetBrains Mono', 'Noto Color Emoji',  'monospace', 'Droid Sans Fallback', 'Material Design Icons', 'Typicons', 'FontAwesome', 'JetBrainsMono NFP'",
  "editor.renderWhitespace": "all",
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "editor.rulers": [120],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "security.workspace.trust.enabled": false,
  "docker.languageserver.formatter.ignoreMultilineInstructions": true,
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "editor.linkedEditing": true,
  "vsicons.dontShowNewVersionMessage": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "bracketPairColorizer.depreciation-notice": false,
  "remote.SSH.remoteServerListenOnSocket": true,
  "glassit.alpha": 220,
  "files.exclude": {
    "**/__pycache__": true,
    "**/.ruff_cache": true
  },
  "workbench.editor.enablePreview": false,
  "editor.lightbulb.enabled": "off",
  "editor.parameterHints.enabled": false,
  "debug.inlineValues": "on",
  "files.autoSave": "onFocusChange",
  "files.associations": {
    "*.txt": "bat",
    "*.py": "python"
  },
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "python.formatting.provider": "none",
  "codesnap.showWindowControls": false,
  "prettier.printWidth": 120,
  "extensions.ignoreRecommendations": true,
  "workbench.tree.enableStickyScroll": false,
  "window.commandCenter": false,
  "git.openRepositoryInParentFolders": "never",
  "editor.hover.delay": 1000,
  "editor.glyphMargin": true,
  "window.restoreWindows": "none",
  "json.maxItemsComputed": 500000,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.defaultInterpreterPath": "/media/hdd/max/public/.venv",
  "editor.wordWrapColumn": 120,
  "database-client.autoSync": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports.ruff": "explicit"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "vscode-great-icons",
  "[sql]": {
    "editor.defaultFormatter": "inferrinizzard.prettier-sql-vscode"
  },
  "python.locator": "js",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "mads-hartmann.bash-ide-vscode"
  },
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "ruff.lineLength": 120,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Tokyo Night",
  "chat.agent.enabled": false,
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "evenBetterToml.formatter.indentString": "",
  "claudeCode.preferredLocation": "panel",
  "terminal.integrated.suggest.upArrowNavigatesHistory": false
}
